@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/AdminCMS/Views/Shared/_Layout.cshtml";
}
@using DataModel.DataViewModel
@model DashboardViewModel

<section class="content">
    <div class="row">
      
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">DashBoard</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-4 col-sm-6 col-xs-12">
                            <div class="info-box">
                                <span class="info-box-icon bg-aqua"><i class="fa fa-cart-plus"></i></span>
                                <div class="info-box-content">
                                    <span class="info-box-number">@Model.NbrTicker</span>
                                    <span class="info-box-text">KÈO</span>
                                </div><!-- /.info-box-content -->
                            </div><!-- /.info-box -->
                        </div><!-- /.col -->
                        <div class="col-md-4 col-sm-6 col-xs-12">
                            <div class="info-box">
                                <span class="info-box-icon bg-red"><i class="fa fa-cart-plus"></i></span>
                                <div class="info-box-content">
                                    <span class="info-box-number">@Model.NbrTickerE</span>
                                    <span class="info-box-text">KÈO ĐANG CHẠY</span>

                                </div><!-- /.info-box-content -->
                            </div><!-- /.info-box -->
                        </div><!-- /.col -->

                        <div class="col-md-3 col-sm-6 col-xs-12">
                            <div class="info-box">
                                <span class="info-box-icon bg-green"><i class="fa fa-cart-plus"></i></span>
                                <div class="info-box-content">
                                    <span class="info-box-number">@Model.NbrTickerP</span>
                                    <span class="info-box-text">KÈO CHỐT LỜI</span>

                                </div><!-- /.info-box-content -->
                            </div><!-- /.info-box -->
                        </div><!-- /.col -->

                    </div><!-- /.row -->
                    <div class="row">
                        <div class="col-md-4 col-sm-6 col-xs-12">
                            <div class="info-box">
                                <span class="info-box-icon bg-aqua"><i class="fa fa-newspaper-o"></i></span>
                                <div class="info-box-content">
                                    <span class="info-box-number">@Model.NbrContentItem</span>
                                    <span class="info-box-text">BÀI VIẾT</span>
                                </div><!-- /.info-box-content -->
                            </div><!-- /.info-box -->
                        </div><!-- /.col -->
                        <div class="col-md-4 col-sm-6 col-xs-12">
                            <div class="info-box">
                                <span class="info-box-icon bg-red"><i class="fa fa-newspaper-o"></i></span>
