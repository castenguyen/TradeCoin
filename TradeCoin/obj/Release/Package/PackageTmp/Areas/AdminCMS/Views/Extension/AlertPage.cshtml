@{
    ViewBag.Title = "Trang thông báo";
    Layout = "~/Areas/AdminCMS/Views/Shared/_MainLayout.cshtml";
}
@model DataModel.DataViewModel.AlertPageViewModel

<body class="hold-transition lockscreen">
    <!-- Automatic element centering -->
    <div class="lockscreen-wrapper">
        <div class="lockscreen-logo">
            <a href="#"><b>PHỐ DU LỊCH</b></a>
        </div>
        <!-- User name -->
        <div class="lockscreen-name">@Model.AlertString</div>
        <div class="text-center">
            @Model.AlertLink
            <a href="@Url.Action("Login", "AccountAdmin")">Vui lòng đăng nhập tại đây</a>
        </div>
        <div class="lockscreen-footer text-center">
            Copyright © 2014-2015 <b><a href="" class="text-black">CMSSHOP</a></b><br>
            All rights reserved
        </div>
    </div><!-- /.center -->
    <!-- jQuery 2.1.4 -->
    <script src="../../plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Bootstrap 3.3.5 -->
    <script src="../../bootstrap/js/bootstrap.min.js"></script>


</body>